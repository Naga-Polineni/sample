<h1> Log in </h1>

<div class = "row" >
  
  <div class = "col-md-6 col-md-offset-3">
    
    <%= form_for @chef do |f|  %>
      <%= f.label :email %>
      <%= f.text_field :email %>
      
      <%= f.label :password %>
      <%= f.text_field :password %>

      
      <%= f.submit("Submit" , class: "btn btn-success") %>
    
    <% end %>
    
  </div>
</div>
